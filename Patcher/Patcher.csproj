    <ItemGroup>
        <PackageReference Include="MessagePack" Version="2.5.140"/>
        <PackageReference Include="System.IO.Hashing" Version="8.0.0"/>
        <PackageReference Include="ZstdSharp.Port" Version="0.8.0" />
    </ItemGroup>

</Project>
